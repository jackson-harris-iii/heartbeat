{
	"name": "heartbeat",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"watch": "nodemon ./src/bin/www",
		"devwin": "set NODE_ENV=development&yarn watch",
		"dev": "NODE_ENV=development&yarn watch",
		"test": "echo no test",
		"test:cover": "yarn test -- --coverage ",
