		"test:watch": "jest --watch",
		"heroku-postbuild": "cd client && yarn && yarn run build"
	},
	"dependencies": {
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"express": "~4.16.0",
		"http-errors": "~1.6.2",
		"jade": "~1.11.0",
		"mongoose": "^5.1.6",
		"morgan": "~1.9.0"
	}
}
